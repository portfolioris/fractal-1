{
  "name": "fractal-1",
  "description": "fractal 1 project files",
  "version": "1.0.0",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "dev": "run-p  vite:serve  static:copy  svg:watch  fractal:start",
    "build": "run-s  vite:build  static:copy  svg:build  fractal:build",
    "vite:serve": "vite --port 30128",
    "vite:build": "vite build  --config scripts/vite.config.js",
    "preview": "vite preview  --config scripts/vite.preview.js",
    "fractal:start": "ENV=development fractal start  --sync",
    "fractal:build": "fractal build",
    "svg:watch": "node scripts/svg.watch.js",
    "svg:build": "node scripts/svg.build.js",
    "static:copy": "node scripts/assets.copy.js"
  },
  "dependencies": {
    "@frctl/fractal": "^1.5.8",
    "@frctl/handlebars": "^1.2.10",
    "@rollup/plugin-dynamic-import-vars": "^1.1.1",
    "@supple-kit/stylelint-config-supple": "^3.0.4",
    "@supple-kit/supple-css": "^5.0.1",
    "conditioner-core": "^2.3.3",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "focus-visible": "^5.2.0",
    "lorem-ipsum": "^2.0.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss-dir-pseudo-class": "^5.0.0",
    "postcss-logical": "^4.0.2",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.3.0",
    "sass": "^1.34.0",
    "scroll-into-view": "^1.15.0",
    "stylelint": "^13.13.1",
    "stylelint-use-logical": "^1.1.0",
    "svgo": "^2.3.0",
    "svgstore-cli": "^2.0.0",
    "vite": "^2.3.4",
    "wicg-inert": "^3.1.1"
  },
  "config": {
    "paths": {
      "source": {
        "docs": "src/docs/",
        "patterns": "src/patterns/",
        "svg": "src/svg/",
        "static": "src/static/"
      },
      "dist": {
        "jsPublicPath": "/js/",
        "svg": "dist/svg/",
        "static": "dist/static/"
      }
    }
  }
}
