    "@supple-kit/stylelint-config-supple": "^3.0.4",
    "@supple-kit/supple-css": "^5.0.2",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "focus-visible": "^5.2.0",
    "lorem-ipsum": "^2.0.4",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "scroll-into-view": "^1.16.0",
    "stylelint": "^13.13.1",
    "stylelint-use-logical": "^1.1.0",
    "svgo": "^2.7.0",
    "svgstore-cli": "^2.0.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.5",
    "wicg-inert": "^3.1.1"
