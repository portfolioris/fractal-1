{
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parser": "@babel/eslint-parser",
  "plugins": [
    "import"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ]
  }
}
